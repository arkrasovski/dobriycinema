{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Film weekly service",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/weekly" : {
      "get" : {
        "description" : "Returns list of cinemas films",
        "operationId" : "weeklyevents",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/FilmsEventsResponce"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "weekly-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "FilmsEventsResponce" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/FilmEvent"
        }
      },
      "FilmEvent" : {
        "required" : [ "date", "image", "name" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string"
          },
          "image" : {
            "type" : "string"
          },
          "description" : {
            "type" : "string"
          },
          "cinemaLocation" : {
            "type" : "string"
          },
          "cinemaName" : {
            "type" : "string"
          },
          "minCost" : {
            "type" : "string"
          },
          "date" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}