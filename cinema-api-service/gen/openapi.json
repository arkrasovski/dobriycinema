{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Music service",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/ratefilm" : {
      "get" : {
        "description" : "Returns list of ratefilm decription",
        "operationId" : "ratefilm",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/RateFilmResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "ratefilm-controller"
    },
    "/recommendations" : {
      "get" : {
        "description" : "Returns list of recommendations films",
        "operationId" : "recommendations",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/RecommendationsResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "recommendations-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "RateFilmResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Film"
        }
      },
      "Film" : {
        "required" : [ "country", "date", "genre", "image", "name", "plot" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "type" : "string"
          },
          "image" : {
            "type" : "string"
          },
          "date" : {
            "type" : "string"
          },
          "genre" : {
            "type" : "string"
          },
          "plot" : {
            "type" : "string"
          },
          "country" : {
            "type" : "string"
          },
          "rating" : {
            "type" : "string"
          }
        }
      },
      "RecommendationsResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Film"
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}